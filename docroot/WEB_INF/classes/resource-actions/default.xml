<?xml version="1.0"?>
<!DOCTYPE resource-action-mapping PUBLIC "-//Liferay//DTD Resource Action Mapping 6.2.0//EN" "http://www.liferay.com/dtd/liferay-resource-action-mapping_6_2_0.dtd">

<resource-action-mapping>
	
	<portlet-resource>
		<portlet-name>service</portlet-name>
		<permissions>
			<supports>
				<action-key>ADD_TO_PAGE</action-key>
				<action-key>CONFIGURATION</action-key>
				<action-key>VIEW</action-key>
			</supports>
			<site-member-defaults>
				<action-key>VIEW</action-key>
			</site-member-defaults>
			<guest-defaults>
				
			</guest-defaults>
			<guest-unsupported />
		</permissions>
	</portlet-resource>
	
	
	<model-resource>
		<model-name>com.lithan.liferay.service.model</model-name>
		<portlet-ref>
			<portlet-name>service</portlet-name>
		</portlet-ref>
		<permissions>
			<supports>
				<action-key>ADD_SERVICE</action-key>
			</supports>
			
			<site-member-defaults></site-member-defaults>
			
			<guest-defaults></guest-defaults>
			
			<guest-unsupported>
				<action-key>ADD_SERVICE</action-key>
			</guest-unsupported>
		</permissions>
	</model-resource>
	

	<model-resource>
		<model-name>com.lithan.liferay.service.model.Servic</model-name>
		<portlet-ref>
			<portlet-name>service</portlet-name>
		</portlet-ref>
		<permissions>
		
			<supports>
				<action-key>DELETE</action-key>
				<action-key>UPDATE</action-key>
				<action-key>PERMISSIONS</action-key>
				<action-key>VIEW</action-key>
			</supports>
			 
			<site-member-defaults>
				<action-key>VIEW</action-key>
			</site-member-defaults>
			
			<guest-defaults>
				
			</guest-defaults>
			
			<guest-unsupported>
				<action-key>DELETE</action-key>
				<action-key>UPDATE</action-key>
				<action-key>PERMISSIONS</action-key>
			</guest-unsupported>
		</permissions>
	</model-resource>
	
</resource-action-mapping>